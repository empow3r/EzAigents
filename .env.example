# EzAugent Environment Configuration
# Copy this file to .env and fill in your values

# Redis Configuration
REDIS_URL=redis://localhost:6379

# API Keys (Get from respective providers)
CLAUDE_API_KEY=sk-ant-api03-xxxxx
OPENAI_API_KEY=sk-xxxxx
DEEPSEEK_API_KEYS=key1,key2,key3
MISTRAL_API_KEY=xxxxx
GEMINI_API_KEY=xxxxx

# OpenRouter Keys (Alternative)
OPENROUTER_API_KEY=sk-or-xxxxx

# Dashboard Configuration
DASHBOARD_PORT=3000
DASHBOARD_HOST=localhost

# Agent Configuration
MAX_AGENTS=100
AGENT_TIMEOUT=300000
AGENT_RETRY_ATTEMPTS=3

# Monitoring
ENABLE_MONITORING=true
METRICS_PORT=9090

# Security
JWT_SECRET=your-secret-key-here
API_RATE_LIMIT=100

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs/ezaugent.log

# Database (if using PostgreSQL/Supabase)
DATABASE_URL=postgresql://user:pass@localhost:5432/ezaugent
SUPABASE_URL=https://xxxxx.supabase.co
SUPABASE_ANON_KEY=xxxxx

# File Paths
FILEMAP_PATH=./shared/filemap.json
TOKENPOOL_PATH=./shared/tokenpool.json

# Environment
NODE_ENV=development