# EzAugent Environment Configuration
# Copy this file to .env and fill in your values

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Primary API Keys (Get from respective providers)
# Claude - https://console.anthropic.com/
CLAUDE_API_KEY=sk-ant-api03-xxxxx
CLAUDE_API_KEY2=sk-ant-api03-xxxxx  # Optional: for rotation
CLAUDE_API_KEY3=sk-ant-api03-xxxxx  # Optional: for rotation

# OpenAI - https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-xxxxx
OPENAI_API_KEY2=sk-xxxxx  # Optional: for rotation
OPENAI_API_KEY3=sk-xxxxx  # Optional: for rotation

# DeepSeek - https://platform.deepseek.com/
DEEPSEEK_API_KEYS=key1,key2,key3,key4  # Comma-separated for rotation

# Mistral - https://console.mistral.ai/
MISTRAL_API_KEY=xxxxx
MISTRAL_API_KEY2=xxxxx  # Optional: for rotation

# Google Gemini - https://makersuite.google.com/app/apikey
GEMINI_API_KEY=xxxxx
GEMINI_API_KEY2=xxxxx  # Optional: for rotation

# OpenRouter Keys (Alternative routing service)
OPENROUTER_API_KEY=sk-or-xxxxx

# API Management
API_KEY_MANAGER_PORT=3001
HEALTH_DASHBOARD_PORT=3002
ROTATION_STRATEGY=health-based  # Options: round-robin, least-used, weighted, health-based

# Dashboard Configuration
DASHBOARD_PORT=3000
DASHBOARD_HOST=localhost

# Agent Configuration
MAX_AGENTS=100
AGENT_TIMEOUT=300000
AGENT_RETRY_ATTEMPTS=3

# Monitoring
ENABLE_MONITORING=true
METRICS_PORT=9090

# Security
JWT_SECRET=your-secret-key-here
API_RATE_LIMIT=100

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs/ezaugent.log

# Database (if using PostgreSQL/Supabase)
DATABASE_URL=postgresql://user:pass@localhost:5432/ezaugent
SUPABASE_URL=https://xxxxx.supabase.co
SUPABASE_ANON_KEY=xxxxx

# File Paths
FILEMAP_PATH=./shared/filemap.json
TOKENPOOL_PATH=./shared/tokenpool.json

# Environment
NODE_ENV=development