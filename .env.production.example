# Ez Aigent Production Environment Configuration
# Copy this file to .env and fill in your actual values

# === API Keys (Required) ===
# Get these from respective AI providers
CLAUDE_API_KEY=sk-or-cl-max-xxxxx
CLAUDE_API_KEY2=sk-or-cl-max-xxxxx
CLAUDE_API_KEY3=sk-or-cl-max-xxxxx

OPENAI_API_KEY=sk-xxxxx
OPENAI_API_KEY2=sk-xxxxx
OPENAI_API_KEY3=sk-xxxxx

DEEPSEEK_API_KEYS=key1,key2,key3,key4
DEEPSEEK_API_KEY2=sk-deepseek-xxxxx
DEEPSEEK_API_KEY3=sk-deepseek-xxxxx
DEEPSEEK_API_KEY4=sk-deepseek-xxxxx

GEMINI_API_KEY=sk-gemini-xxxxx
GEMINI_API_KEY2=sk-gemini-xxxxx

MISTRAL_API_KEY=sk-mistral-xxxxx

OPENROUTER_API_KEY=sk-or-xxxxx
OPENROUTER_API_KEY2=sk-or-xxxxx

# === Database & Authentication ===
# Supabase configuration
DATABASE_URL=postgresql://user:password@host:5432/database
SUPABASE_URL=https://yourproject.supabase.co
SUPABASE_ANON_KEY=your-anon-key

# JWT secret for session management
JWT_SECRET=your-secure-random-string-at-least-32-chars

# === SSL Configuration ===
# Domain for SSL certificates
SSL_CERT_DOMAIN=yourdomain.com

# === SMS/Voice Configuration (Optional) ===
# Telnyx API credentials
TELNYX_API_KEY=KEY_xxxxx
TELNYX_FROM_NUMBER=+1234567890
TELNYX_MESSAGING_PROFILE_ID=xxxxx
TELNYX_WEBHOOK_URL=https://yourdomain.com/api/telnyx-webhook
TELNYX_PUBLIC_KEY=xxxxx

# Admin phone numbers (comma-separated)
ADMIN_PHONE_NUMBERS=+1234567890,+0987654321

# === Notification Services (Optional) ===
# Slack webhook for alerts
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/xxxxx

# Email notification settings
EMAIL_NOTIFICATION=true
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# === External Services (Optional) ===
# GitHub integration
GITHUB_TOKEN=ghp_xxxxx
GITHUB_REPO=your-org/your-repo

# AWS S3 for storage
AWS_ACCESS_KEY_ID=AKIA_xxxxx
AWS_SECRET_ACCESS_KEY=xxxxx
S3_BUCKET_NAME=ez-aigent-storage

# Error tracking
SENTRY_DSN=https://xxxxx@sentry.io/xxxxx

# === Security ===
# Emergency shutdown key for admin operations
EMERGENCY_SHUTDOWN_KEY=emergency-shutdown-secret-key

# Admin contact email
ADMIN_CONTACT=admin@yourdomain.com