{
  "redis": {
    "cluster": {
      "enabled": false,
      "nodes": [
        { "host": "redis-1", "port": 6379 },
        { "host": "redis-2", "port": 6380 },
        { "host": "redis-3", "port": 6381 }
      ]
    },
    "single": {
      "host": "localhost",
      "port": 6379,
      "password": null
    },
    "options": {
      "maxRetriesPerRequest": 3,
      "retryDelayOnFailover": 100,
      "enableOfflineQueue": false,
      "keepAlive": 30000,
      "connectTimeout": 10000,
      "commandTimeout": 5000
    }
  },
  "agents": {
    "scaling": {
      "maxAgents": 1000,
      "agentsPerCore": 50,
      "autoScale": true,
      "scaleUpThreshold": 80,
      "scaleDownThreshold": 20
    },
    "performance": {
      "queueTimeout": 5000,
      "heartbeatInterval": 10000,
      "metricsInterval": 30000,
      "errorBackoffMax": 30000,
      "taskTimeout": 300000
    },
    "resources": {
      "maxMemoryPerAgent": "256MB",
      "maxCpuPerAgent": 0.5,
      "diskQuotaPerAgent": "1GB"
    }
  },
  "models": {
    "claude-3-opus": {
      "endpoint": "https://api.openrouter.ai/v1/chat/completions",
      "maxTokens": 2048,
      "temperature": 0.3,
      "timeout": 30000,
      "retries": 3,
      "rateLimit": {
        "requestsPerMinute": 100,
        "tokensPerMinute": 50000
      }
    },
    "gpt-4o": {
      "endpoint": "https://api.openai.com/v1/chat/completions",
      "maxTokens": 2048,
      "temperature": 0.3,
      "timeout": 30000,
      "retries": 3,
      "rateLimit": {
        "requestsPerMinute": 150,
        "tokensPerMinute": 75000
      }
    },
    "deepseek-coder": {
      "endpoint": "https://api.openrouter.ai/v1/chat/completions",
      "maxTokens": 1024,
      "temperature": 0.2,
      "timeout": 20000,
      "retries": 3,
      "rateLimit": {
        "requestsPerMinute": 200,
        "tokensPerMinute": 100000
      }
    },
    "command-r-plus": {
      "endpoint": "https://api.openrouter.ai/v1/chat/completions",
      "maxTokens": 1024,
      "temperature": 0.3,
      "timeout": 25000,
      "retries": 3,
      "rateLimit": {
        "requestsPerMinute": 120,
        "tokensPerMinute": 60000
      }
    }
  },
  "monitoring": {
    "metrics": {
      "enabled": true,
      "retention": "7d",
      "aggregation": "1m",
      "export": {
        "prometheus": true,
        "grafana": true,
        "json": true
      }
    },
    "alerts": {
      "enabled": true,
      "channels": ["console", "redis"],
      "thresholds": {
        "errorRate": 5,
        "responseTime": 5000,
        "memoryUsage": 80,
        "queueDepth": 1000
      }
    },
    "healthChecks": {
      "interval": 30000,
      "timeout": 5000,
      "retries": 3
    }
  },
  "filePatterns": [
    {
      "pattern": "\\.(js|ts)$",
      "model": "claude-3-opus",
      "prompt": "Refactor and optimize this code for performance and maintainability.",
      "priority": "high"
    },
    {
      "pattern": "\\.(jsx|tsx)$",
      "model": "claude-3-opus", 
      "prompt": "Optimize React component for performance and best practices.",
      "priority": "high"
    },
    {
      "pattern": "\\.py$",
      "model": "deepseek-coder",
      "prompt": "Optimize Python code for performance and add type hints.",
      "priority": "medium"
    },
    {
      "pattern": "test.*\\.(js|ts|py)$",
      "model": "deepseek-coder",
      "prompt": "Create comprehensive test coverage for this code.",
      "priority": "high"
    },
    {
      "pattern": "README\\.md$",
      "model": "command-r-plus",
      "prompt": "Generate comprehensive documentation.",
      "priority": "low"
    },
    {
      "pattern": "\\.(yaml|yml|json)$",
      "model": "gpt-4o",
      "prompt": "Validate and optimize configuration file.",
      "priority": "medium"
    },
    {
      "pattern": "Dockerfile",
      "model": "gpt-4o",
      "prompt": "Optimize Docker configuration for production deployment.",
      "priority": "high"
    },
    {
      "pattern": "package\\.json$",
      "model": "claude-3-opus",
      "prompt": "Optimize dependencies and scripts for performance.",
      "priority": "high"
    }
  ],
  "caching": {
    "configs": {
      "ttl": 300000,
      "maxSize": 100
    },
    "api": {
      "ttl": 60000,
      "maxSize": 1000
    },
    "files": {
      "ttl": 600000,
      "maxSize": 500
    }
  },
  "optimization": {
    "bundling": {
      "enabled": true,
      "splitChunks": true,
      "treeShaking": true,
      "minification": true
    },
    "compression": {
      "enabled": true,
      "algorithm": "gzip",
      "level": 6
    },
    "caching": {
      "staticAssets": "1y",
      "apiResponses": "5m",
      "dynamicContent": "1h"
    }
  },
  "security": {
    "rateLimiting": {
      "enabled": true,
      "windowMs": 60000,
      "maxRequests": 1000
    },
    "validation": {
      "strictMode": true,
      "sanitizeInputs": true,
      "validateOutputs": true
    },
    "encryption": {
      "transit": true,
      "rest": false
    }
  }
}